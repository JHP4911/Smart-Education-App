<MainWindow>:
    id: main_win
    orientation: "vertical"
    spacing: 10
    space_x: self.size[0]/3


    canvas.before:
        Color:
            rgba: (1,1,1, 1)
        Rectangle:
            source:'work.jpg'
            size: root.width-400,root.height
            pos: self.pos

    BoxLayout:
        size_hint_y: None
        height: 50
        canvas.before:
            Color:
                rgba: (.06, .5, .5, 1)
            Rectangle:
                size: self.size
                pos: self.pos
        Label:
            text: "SMART TEACHER APP"
            bold: True
            size_hint_x: .9
    BoxLayout:
        orientation: 'vertical'
        padding: main_win.space_x, 10
        #spacing: 20
        Label:
            id: sp21

        Button:
            text: "Login"
            pos_hint:{'center_x': 1.25, 'center_y': 1}
            size_hint_y: None
            height: 40
            background_color: (.06, .45, .45, 1)
            background_normal: ''
            on_release:root.li()


        Label:
            id: sp2
            spacing:10

        Button:
            text :"Sign In"
            pos_hint:{'center_x': 1.25, 'center_y': 1}
            size_hint_y: None
            height: 40
            background_color: (.06, .45, .45, 1)
            background_normal: ''
            on_release:root.si()
        Label:
            id :sp4
        Label:
            id :sp5
        Button:
            text :"EXIT"
            pos_hint:{'center_x': 1.25, 'center_y': 1}
            size_hint_y: None
            height: 40
            background_color: (.06, .45, .45, 1)
            background_normal: ''
            on_press:app.stop()
